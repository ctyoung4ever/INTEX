<p><strong>Burial ID:</strong> @Model.BurialMain.Id</p>
<tbody>
    <!-- Access other burial main properties as needed -->


    @foreach (var i in Model.Textile)
    {
        <tr>
            <td>@i.Id</td>
            <td>@i.Locale</td>
            <td>@i.Description</td>
            <td>@i.Textileid</td>
            <td>@i.Burialnumber</td>
            <td>@i.Estimatedperiod</td>

        </tr>
        <br />
    }

    @if (Model.BodyAnalysis != null)
    {
        <td><a class="nav-link text-dark" asp-area="" asp-route-burialmainid="@Model.BodyAnalysis.Id" asp-controller="Home" asp-action="EditBodyAnalysis">Edit</a></td>
        <td><a asp-controller="Home" asp-action="DeleteBodyAnalysis" asp-route-id="@Model.BodyAnalysis.Id">Delete</a></td>
        <tr>
            <td>@Model.BodyAnalysis.Id</td>
            <td>@Model.BodyAnalysis.Squarenorthsouth</td>
        </tr>

        <br />
    }
    else
    {
        <a class="nav-link text-dark" asp-area="" asp-route-burialmainid="@Model.BurialMain.Id" asp-controller="Home" asp-action="CreateBodyAnalysis">Create</a>
    }
    @foreach (var i in Model.Photos)
    {
<tr>
    <td>@i.Id</td>
    <td>@i.Description</td>
    <a class="nav-link text-dark" asp-area="" asp-route-pictureid="@i.Id" asp-controller="Home" asp-action="EditPicture">Edit Photo</a>
    <td><a asp-controller="Home" asp-action="DeletePicture" asp-route-id="@i.Id">Delete Item @i.Id</a></td>
    <h1>Photos</h1>
</tr>
        <br />
    }

    @if (Model.C14 != null)
    {
        <tr>
            <td>@Model.C14.Id</td>
            <td>@Model.C14.Agebp</td>
        </tr>
    }
    }
</tbody>
